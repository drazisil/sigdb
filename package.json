  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drazisil/sigdb/issues"
  },
  "homepage": "https://github.com/drazisil/sigdb#readme",
  "devDependencies": {
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.0",
    "prettier": "2.3.2",
    "tap": "15.0.9"
  },
  "lint-staged": {
    "**/*.js": [
      "npx prettier --write",
